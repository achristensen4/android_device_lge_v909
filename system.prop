#
# system.prop for LGE V909
#
ro.cwm.prefer_tar=true
ro.opengles.version=131072
wifi.interface=wlan0
ro.ril.wake_lock_timeout=180000
rild.libpath=/system/lib/lge-ril.so
rild.libargs=-d /dev/ttyspi0
sys.settings_system_version=2
wifi.supplicant_scan_interval=16
drm.service.enabled=1
keyguard.no_require_sim=no

# We can afford the 64m
dalvik.vm.heapsize=64m

# hold precise GC data, and use SMP
dalvik.vm.dexopt-flags=m=y,u=y

# GSM, WCDMA preferred
# This appears to be causing "Unknown" network type
#ro.telephony.default_network=4
